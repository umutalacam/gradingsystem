<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="189.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.EditCourseDialog">
   <children>
      <SwingNode fx:id="node_operation" />
      <SwingNode fx:id="node_instructor_id" />
      <SwingNode fx:id="node_course_id" />
      <TextField fx:id="field_course_name" layoutX="137.0" layoutY="52.0" prefHeight="26.0" prefWidth="230.0" promptText="Course Name" />
      <TextField fx:id="field_quota" layoutX="137.0" layoutY="82.0" prefHeight="26.0" prefWidth="230.0" promptText="Quota" />
      <Button layoutX="311.0" layoutY="150.0" mnemonicParsing="false" onAction="#applyAction" text="Apply" />
      <Button layoutX="243.0" layoutY="150.0" mnemonicParsing="false" onAction="#cancelAction" text="Cancel" />
      <ImageView fitHeight="111.0" fitWidth="110.0" layoutX="14.0" layoutY="23.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../course_icon.png" />
         </image>
      </ImageView>
      <TextField fx:id="field_course_id" layoutX="137.0" layoutY="23.0" prefHeight="26.0" prefWidth="230.0" promptText="Course ID" />
   </children>
